{
  "name": "UsableNet PageCapture for automated tools",
  "version": "1.2",
  "manifest_version": 3,
  "key": "",
  "description": "Capture web pages to be processed by UsableNet AQA from automated testing tools",
  "author": "UsableNet",
  "icons": {
    "128": "icons/icon-128x128.png"
  },
  "background": {
    "service_worker": "background/background.bundle.js"
  },
  "host_permissions": [
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "contentscripts/overrides-init.js"
      ],
      "run_at": "document_start",
      "all_frames": true,
      "match_about_blank": true,
      "world": "MAIN"
    },
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "contentscripts/snapshotter.bundle.js"
      ],
      "run_at": "document_end",
      "all_frames": true,
      "match_about_blank": true,
      "match_origin_as_fallback": true
    },
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "contentscripts/contentscripts.bundle.js"
      ],
      "run_at": "document_end",
      "all_frames": true,
      "match_about_blank": true,
      "match_origin_as_fallback": true
    },
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "contentscripts/msgFallback.js"
      ]
    }
  ],
  "permissions": [
    "storage",
    "webNavigation",
    "scripting",
    "pageCapture",
    "unlimitedStorage"
  ],
  "externally_connectable":{
    "matches": ["<all_urls>"]
  }
}
